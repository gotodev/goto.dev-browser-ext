{
  "manifest_version": 2,
  "name": "goto.dev",
  "version": "2021.04.22",
  "version_name": "2021.04.22",

  "description": "Understand code 10x faster on GitHub ðŸ˜‰",
  "author": "goto.dev",
  "homepage_url": "https://goto.dev",
  "icons": {
    "16": "img/icon-16.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },

  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },

  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": ["https://github.com/*"],
      "js": [
        "vendor/popper.v2.9.2.js",
        "vendor/tippy.umd.v6.3.1.js",
        "vendor/prism.v1.23.0.js",
        "content.js"
      ],
      "css": [
        "vendor/tippy.v6.3.1.css",
        "prism.css",
        "style.css"
      ]
    }
  ]
}
